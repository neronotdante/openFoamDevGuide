      "command": [
        "cd ${workspaceFolder};",
        "source /opt/openfoam10/etc/bashrc WM_NCOMPROCS=2;",
        "wmake 2>&1 | tee log.wmake_opt"
      ],
      "args": [],
      "options": {},
      "problemMatcher": [],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "type": "shell",
      "label": "ofextension: debug solver",
      "command": [
        "cd ${workspaceFolder};",
        "source /opt/openfoam10/etc/bashrc WM_NCOMPROCS=2; export WM_COMPILE_OPTION=Debug;",
        "wmake 2>&1 | tee log.wmake_debug"
      ],
      "args": [],
      "options": {},
      "problemMatcher": []
    }
  ],
